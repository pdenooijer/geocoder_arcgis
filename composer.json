{
  "name": "Geocoder Arcgis",
  "description": "Provides a CTools plugin for the Geocoder module that translates an address to a geolocation by using ArcGIS.",
  "license": "GPLv3",
  "require-dev": {
    "drupal/coder": "^8.2",
    "phayes/geophp": "^1.2",
    "phpunit/phpunit": "^5.7",
    "drupal/drupal": "^7.53",
    "drupal/geocoder": "^1.3"
  },
  "autoload" : {
    "psr-4": {
      "Drupal\\geocoder_arcgis\\": "src/"
    }
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/7"
    }
  }
}
